{
  "name": "sail-boat-engine",
  "version": "0.0.1",
  "description": "Gives you the speed and radius of a sailboat with a certain electric/diesel engine",
  "main": "SailBoatEngine.js",
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && jsdoc -c jsdoc.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thehaguefreek/sail-boat-engine-lib.git"
  },
  "author": "Freek Olgers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thehaguefreek/sail-boat-engine-lib/issues"
  },
  "homepage": "https://github.com/thehaguefreek/sail-boat-engine-lib#readme",
  "dependencies": {
    "jaguarjs-jsdoc": "^1.1.0"
  }
}
